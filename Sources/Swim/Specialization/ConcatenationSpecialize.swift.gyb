%{
from type import *
}%

% for pt in pixel_types:
% for dt in bool_data_types + int_data_types + fp_data_types:
public func concatH(_ images: [Image<${pt}, ${dt}>]) -> Image<${pt}, ${dt}> {
    return _concatH(images)
}

public func concatV(_ images: [Image<${pt}, ${dt}>]) -> Image<${pt}, ${dt}> {
    return _concatV(images)
}

% end
% end